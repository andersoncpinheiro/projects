<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Produtos</title>
		</head>
	<body>

		<h1>Produtos</h1>

		<table>
			
			<tr>
				<td>ID</td>
				<td>Descricao</td>
				<td>Fabricante</td>
				<td>Valor Unit√°rio</td>

			</tr>
			<% for(var i = 0; i < produtos.length; i++){ %>
				<tr>
					<td><%= produtos[i].id_produto %></td>
					<td><%= produtos[i].descricao %></td>
					<td><%= produtos[i].fabricante %></td>
					<td><%= produtos[i].valor_uni %> R$</td>
				</tr>
			<% } %>

		</table>

	</body>
</html>